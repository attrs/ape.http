{
	"preferences": {
		"plexi.http": {
			"servers": {
				"front": {
					"port": 9090,
					"session": {
						"resave": true,
						"saveUninitialized": true,
						"secret": "tlzmflt"
					},
					"logging": {
						"access": ":date - :method :status :url :remote-addr [:res[content-length] :referrer :user-agent :response-time ms]",
						"error": ":date - :method :status :url :remote-addr [:res[content-length] :referrer :user-agent :response-time ms]",
						"console": "error",
						"web": true
					},
					"docbase": {
						"commons": "www/commons",
						"mobile": "www/mobile",
						"*": "www/web"
					},
					"bower": "libs",
					"statuspage": true,
					"vhost": [
						{
							"host": "m.attrs.io",
							"docbase": {
								"commons": "www/commons",
								"*": "www/mobile"
							},
							"bower": true
						}, {
							"host": "*.attrs.io",
							"docbase": "www/:1",
							"bower": false,
							"statuspage": false,
							"inherit": false
						}, {
						
							"host": "admin.attrs.io",
							"forward": 9081
						}, {
							"host": "forward.attrs.io",
							"forward": "http://google.com"
						}
					],
					"mapping": {
						"pluginname*": "m.attrs.io",
						"pluginname@version*": "*.attrs.io",
						"pluginname@*:bucketname": ["admin.attrs.io", "forward.attrs.io"],
						"*": "*"
					}
				},
				"admin": {
					"port": 9091,
					"docbase": "www/admin",
					"mapping": {
						"plexi.workbench": "*"
					}
				}
			}
		}
	}
}